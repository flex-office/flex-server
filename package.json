{
  "name": "FlexOffice",
  "version": "0.4.0",
  "private": true,
  "main": "server.js",
  "engines": {
    "node": "10.15.3"
  },
  "scripts": {
    "start": "node lib/server.js",
    "build": "npx mocha; npx chai; npx chai-http; npm install ;./node_modules/typescript/bin/tsc -p .",
    "lint": "eslint app --ext ts",
    "lint:fix": "eslint app --ext ts --fix",
    "build:live": "nodemon --watch 'app/**/*.ts' --exec 'ts-node' ./app/server.ts",
    "test": "mocha -r ts-node/register app/test/mocha/*.ts",
    "test-jest": "jest",
    "test:typescript": "npx jest",
    "build:go": "node-gyp configure build",
    "compile:go": "go build -buildmode=c-shared -o crypto.so crypto-go/golang/crypto.go",
    "docker": "if test \"$DOCKER_ENV\" = \"production\" ; then npm start ; else npm run build:live ; fi "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BREDFactory/flex-server"
  },
  "author": "rioflan",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^23.3.5",
    "@types/node": "^10.14.16",
    "@types/node-mailjet": "^3.3.0",
    "app-root-path": "^3.0.0",
    "assert": "^2.0.0",
    "azure-storage": "^2.10.3",
    "base64-img": "^1.0.4",
    "bcryptjs": "latest",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "dredd": "^9.0.5",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-typescript": "^0.12.0",
    "express": "~4.16.0",
    "express-http-context": "^1.2.3",
    "express-sslify": "^1.2.0",
    "greenlock-express": "^2.4.6",
    "gridfs-stream": "^1.1.1",
    "http-errors": "~1.6.2",
    "image-to-base64": "^2.0.1",
    "jest": "^25.0.0",
    "jsonwebtoken": "latest",
    "mockgoose": "^8.0.4",
    "moment": "^2.24.0",
    "mongodb-memory-server": "^4.2.1",
    "mongoose": "^5.6.12",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "node-gyp": "^3.8.0",
    "node-mailjet": "^3.3.1",
