    "nodemon": "^1.18.3",
    "ramda": "^0.25.0",
    "randombytes": "^2.0.6",
    "redis": "^2.8.0",
    "socket.io": "^2.2.0",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
